/** TOPIC / SERVICES DEFINITION
*
*  TOPIC(type, topic, source, dest, priority, period, time_to_live)
*  QOSTOPIC(type, topic, source, dest, priority, period, time_to_live, queue_length)
*  TFTOPIC(topic, source, dest, priority, period, time_to_live)
*
*  SERVICE(type, topic, source, priority, time_to_live)
*
*/

    TOPIC(std_msgs::Int32,      "int",      0, "0,1", 75, 1000, 3000);
    TOPIC(std_msgs::Float64,    "float",    0, "0", 76, 1000, 3000);
    QOSTOPIC(std_msgs::Float64, "qos_test", 0, "0", 77, 100,  3000, 10);
    TOPIC(ros_packet_msgs::packet,      "laser",      0, "2", 10, 1, 500);
    TOPIC(ros_packet_msgs::packet,      "image",      0, "2", 9,  1, 500);
    TOPIC(ros_packet_msgs::Controller,   "u",          2, "0", 19,  1, 500);
    TOPIC(ros_packet_msgs::Controller,   "y",          0, "2", 19,  1, 500);
    TOPIC(ros_packet_msgs::packet,      "laser2",      2, "0", 10, 1, 500);
    TOPIC(ros_packet_msgs::packet,      "image2",      2, "0", 9,  1, 500);

